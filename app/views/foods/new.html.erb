<%= render 'shared/navigation' %>
<div class ="form">
<h1>Add Food</h1>
<%= form_with(model: @food, url: user_foods_path) do |form| %>
      <%= form.label :name %>
      <%= form.text_field :name %>
      <%= form.label :measurement_unit%>
      <%= form.text_field :measurement_unit %>
      <%= form.label :price %>
      <%= form.text_field :price %>
      <%= form.label :quantity %>
      <%= form.text_field :quantity %>
      <%= form.submit 'Submit' %>
  <% end %>
  </div>